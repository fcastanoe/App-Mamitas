    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <!-- Contorno del marco de imagen -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#FF000000"
        android:strokeWidth="2"
        android:strokeLineJoin="round"
        android:pathData="M3,3 H21 V21 H3 Z" />

    <!-- Línea interna que simula un paisaje (montañas) -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#FF000000"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:pathData="M3,21 L8,14 L13,18 L21,10" />

    <!-- Círculo pequeño que sugiere el sol -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#FF000000"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:pathData="M16,6 m-1,0 a1,1 0 1,0 2,0 a1,1 0 1,0 -2,0" />

    <group
        android:translateX="8"
        android:translateY="13"
        android:scaleX="0.8"
        android:scaleY="0.8">

        <!-- Círculo de la lupa -->
        <path
            android:fillColor="@android:color/transparent"
            android:strokeColor="#E6000000"
            android:strokeWidth="2"
            android:pathData="M2,2 a6,6 0 1,0 12,0 a6,6 0 1,0 -12,0" />

        <!-- Mango de la lupa (línea diagonal) -->
        <path
            android:fillColor="@android:color/transparent"
            android:strokeColor="#E6000000"
            android:strokeWidth="2"
            android:pathData="M12,6 L30,25" />


    </group>
